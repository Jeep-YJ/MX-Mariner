CREATE TABLE regions ( 
    name          TEXT,
    description   TEXT,
    image         TEXT,
    size          INT,
    installeddate INT,
    latestdate    INT 
);
CREATE TABLE charts ( 
    region  TEXT,
    file    TEXT,
    name    TEXT,
    updated TEXT,
    scale   INT,
    outline TEXT,
    depths  TEXT 
);
